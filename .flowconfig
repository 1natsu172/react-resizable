[ignore]
<PROJECT_ROOT>/node_modules/react/.*
<PROJECT_ROOT>/node_modules/babel.*
<PROJECT_ROOT>/node_modules/fbjs/.*
<PROJECT_ROOT>/node_modules/styled-components/src/.*
<PROJECT_ROOT>/node_modules/express/.*
<PROJECT_ROOT>/node_modules/serve-index/.*
.*test

[include]
./lib
index.js

[libs]

[options]
suppress_comment=.*\\\\s*$FlowFixMe.*
suppress_comment=.*\\\\s*$FlowBug.*
suppress_comment=.*\\\\s*$FlowIgnore.*
suppress_comment=\\(.\\|\n\\)*\\$FlowNewLine.*
suppress_comment=\\(.\\|\n\\)*\\$FlowIssue
esproposal.class_instance_fields=ignore
esproposal.class_static_fields=ignore
